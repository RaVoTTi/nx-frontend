{
  "projectType": "library",
  "sourceRoot": "libs/book-base/src",
  "prefix": "frontend",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/book-base"],
      "options": {
        "jestConfig": "libs/book-base/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/book-base/**/*.ts",
          "libs/book-base/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
